<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EDSDoctorsApp.Pages.RegWizardPage"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout>
            <WebView
                x:Name="wvTest"
                HorizontalOptions="FillAndExpand"
                VerticalOptions="FillAndExpand" />
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Button BackgroundColor="White" TextColor="Black" Text="Cancel" HorizontalOptions="Center" Clicked="GoBack" />
                <Button BackgroundColor="White" TextColor="Black" Text="Next" HorizontalOptions="Center" Grid.Column="1" Clicked="Continue_Registration" />
            </Grid>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>